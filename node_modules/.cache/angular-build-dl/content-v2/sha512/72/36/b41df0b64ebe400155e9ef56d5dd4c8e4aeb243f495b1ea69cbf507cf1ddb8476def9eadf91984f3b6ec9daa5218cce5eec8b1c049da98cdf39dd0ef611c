    { type: _data_settings__WEBPACK_IMPORTED_MODULE_6__["Settings"] },
    { type: _data_product__WEBPACK_IMPORTED_MODULE_7__["Product"] },
    { type: _ionic_angular__WEBPACK_IMPORTED_MODULE_2__["LoadingController"] },
